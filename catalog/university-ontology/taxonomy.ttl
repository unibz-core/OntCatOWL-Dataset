@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/ActiveClass> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Class> .

<http://taxonomy.model/ActiveCurriculum> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Curriculum> .

<http://taxonomy.model/ActiveProfessor> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Professor> .

<http://taxonomy.model/Center> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Organization> .

<http://taxonomy.model/CourseCoordinator> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Professor> .

<http://taxonomy.model/Department> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Organization> .

<http://taxonomy.model/DepartmentChief> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Professor> .

<http://taxonomy.model/EgressStudent> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Student> .

<http://taxonomy.model/Higher_Education_Institution> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Organization> .

<http://taxonomy.model/InactiveClass> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Class> .

<http://taxonomy.model/InactiveCurriculum> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Curriculum> .

<http://taxonomy.model/InactiveProfessor> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Professor> .

<http://taxonomy.model/RegularStudent> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Student> .

<http://taxonomy.model/SuspendedStudent> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Student> .

<http://taxonomy.model/Class> a owl:Class .

<http://taxonomy.model/Curriculum> a owl:Class .

<http://taxonomy.model/Person> a owl:Class .

<http://taxonomy.model/Organization> a owl:Class .

<http://taxonomy.model/Student> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Person> .

<http://taxonomy.model/Professor> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Person> .

