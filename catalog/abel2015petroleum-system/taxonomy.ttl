@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Oil> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Hidrocarbon> .

<http://taxonomy.model/Reservoir> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/LithologicUnit_qua_sandstone> .

<http://taxonomy.model/Sandstone> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Siliciclastic_Rock> .

<http://taxonomy.model/Seal> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/LithologicUnit_qua_shale> .

<http://taxonomy.model/Shale> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Siliciclastic_Rock> .

<http://taxonomy.model/Source_Rock> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/LithologicUnit_qua_shale> .

<http://taxonomy.model/Hidrocarbon> a owl:Class .

<http://taxonomy.model/LithologicUnit_qua_sandstone> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Lithological_unit> .

<http://taxonomy.model/Rock> a owl:Class .

<http://taxonomy.model/LithologicUnit_qua_shale> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Lithological_unit> .

<http://taxonomy.model/Lithological_unit> a owl:Class .

<http://taxonomy.model/Siliciclastic_Rock> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Rock> .

