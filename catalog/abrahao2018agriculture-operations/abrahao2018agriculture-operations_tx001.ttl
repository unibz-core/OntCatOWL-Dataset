@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Company> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Organization> .

<http://taxonomy.model/Worker> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/PhysicalAgent> .

<http://taxonomy.model/Organization> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/SocialAgent> .

<http://taxonomy.model/PhysicalAgent> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Agent> .

<http://taxonomy.model/SocialAgent> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Agent> .

<http://taxonomy.model/Agent> a owl:Class .

