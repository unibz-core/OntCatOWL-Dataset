@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Action> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Event> .

<http://taxonomy.model/Consumable> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Object> .

<http://taxonomy.model/DigitalGame> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Match> .

<http://taxonomy.model/Equipment> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Object> .

<http://taxonomy.model/Etc> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Object> .

<http://taxonomy.model/NonPlayerCharacter> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Entity> .

<http://taxonomy.model/Player> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Character> .

<http://taxonomy.model/Scenario> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Location> .

<http://taxonomy.model/Character> a owl:Class .

<http://taxonomy.model/Event> a owl:Class .

<http://taxonomy.model/Location> a owl:Class .

<http://taxonomy.model/Match> a owl:Class .

<http://taxonomy.model/Entity> a owl:Class .

<http://taxonomy.model/Object> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Entity> .

