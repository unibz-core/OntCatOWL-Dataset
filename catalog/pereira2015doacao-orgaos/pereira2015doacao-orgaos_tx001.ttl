@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Coração> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Órgão> .

<http://taxonomy.model/Córnea> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Tecido> .

<http://taxonomy.model/Funcional> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Órgão> .

<http://taxonomy.model/Item_Transplantado> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Item_Captado> .

<http://taxonomy.model/Não_Funcional> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Órgão> .

<http://taxonomy.model/Tecido> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Item_Captado> .

<http://taxonomy.model/Item_Captado> a owl:Class .

<http://taxonomy.model/Órgão> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Item_Captado> .

