@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/CollaborationArtifact> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Artifact> .

<http://taxonomy.model/CollaborativeAction> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Participation> .

<http://taxonomy.model/NonVerbalLanguage> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Language> .

<http://taxonomy.model/Participant> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Agent> .

<http://taxonomy.model/RealSite> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Site> .

<http://taxonomy.model/Receive> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Participation> .

<http://taxonomy.model/Send> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Participation> .

<http://taxonomy.model/VerbalLanguage> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Language> .

<http://taxonomy.model/VirtualSite> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Site> .

<http://taxonomy.model/Agent> a owl:Class .

<http://taxonomy.model/Artifact> a owl:Class .

<http://taxonomy.model/Language> a owl:Class .

<http://taxonomy.model/Site> a owl:Class .

<http://taxonomy.model/Participation> a owl:Class .

