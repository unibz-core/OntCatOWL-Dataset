@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Business_Role> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Role> .

<http://taxonomy.model/Employee> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Person> .

<http://taxonomy.model/Program_increment> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Software_development_project> .

<http://taxonomy.model/Programming_environment> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Resource> .

<http://taxonomy.model/Team> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Resource> .

<http://taxonomy.model/Person> a owl:Class .

<http://taxonomy.model/Role> a owl:Class .

<http://taxonomy.model/Software_development_project> a owl:Class .

<http://taxonomy.model/Resource> a owl:Class .

