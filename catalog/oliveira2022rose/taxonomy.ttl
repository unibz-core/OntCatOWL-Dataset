@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://taxonomy.model/Attack> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Threat_Event>,
        <http://taxonomy.model/UFO::Action> .

<http://taxonomy.model/Attacker> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Threat_Object>,
        <http://taxonomy.model/UFO::Agent> .

<http://taxonomy.model/Control_Chain_Event> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Protection_Event> .

<http://taxonomy.model/Control_Event> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Protection_Event> .

<http://taxonomy.model/Experience_Risk_Assesment> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Risk_Assessment> .

<http://taxonomy.model/Experience_Value_Asc.> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Value_Ascription> .

<http://taxonomy.model/Generic_Intention> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Intention> .

<http://taxonomy.model/Impact_Event> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Value_Event> .

<http://taxonomy.model/Loss_Event> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Risk_Event> .

<http://taxonomy.model/Loss_Event_Type> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Risk_Event_Type> .

<http://taxonomy.model/Object_Risk_Assesment> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Risk_Assessment> .

<http://taxonomy.model/Object_Value_Asc.> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Value_Ascription> .

<http://taxonomy.model/Risk_Assessor> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/UFO::Agent> .

<http://taxonomy.model/Risk_Subject> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/UFO::Agent> .

<http://taxonomy.model/Specific_Intention> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Intention> .

<http://taxonomy.model/Threat_Event_Type> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Risk_Event_Type> .

<http://taxonomy.model/Trigger_Event> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Value_Event> .

<http://taxonomy.model/Value_Subject> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/UFO::Agent> .

<http://taxonomy.model/Threat_Event> a owl:Class ;
    rdfs:subClassOf <http://taxonomy.model/Risk_Event> .

<http://taxonomy.model/Threat_Object> a owl:Class .

<http://taxonomy.model/UFO::Action> a owl:Class .

<http://taxonomy.model/Intention> a owl:Class .

<http://taxonomy.model/Protection_Event> a owl:Class .

<http://taxonomy.model/Risk_Assessment> a owl:Class .

<http://taxonomy.model/Risk_Event> a owl:Class .

<http://taxonomy.model/Risk_Event_Type> a owl:Class .

<http://taxonomy.model/Value_Ascription> a owl:Class .

<http://taxonomy.model/Value_Event> a owl:Class .

<http://taxonomy.model/UFO::Agent> a owl:Class .

